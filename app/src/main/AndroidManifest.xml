        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name=".updateversion.UpdateVersionService" />

        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EnterActivity" />
        <activity android:name=".ApprovalActivity" />
        <activity android:name=".MyStudentActivity" />
        <activity android:name=".SiNong.SApproveActivity" />
        <activity android:name=".BaoGe.BSearchActivity" />
        <activity android:name=".BaoGe.BSearch_DetailsActivity" />
        <activity android:name=".XiaoHui.XApprovepeopleActivity" />
        <activity android:name=".BaoGe.BStudent_ShowActivity" />
        <activity android:name=".SiNong.SMylaunchActivity" />
        <activity android:name=".SiNong.SStudentEditActivity" />
        <activity android:name=".XiaoHui.XLoginActivity" />
        <activity android:name=".XiaoHui.XApprovalDetailsActivity" />
        <activity android:name=".SiNong.StulistActivity" />
        <activity android:name=".BaoGe.XAdd_speopleActivity" />
        <activity android:name=".SiNong.SDepartmentActivity" />
        <activity android:name=".SiNong.SDivision" />
        <activity android:name=".SiNong.SBranchActivity" />
        <activity android:name=".RegionActivity" />
        <activity android:name=".SiNong.SApproveTeacherList" />
        <activity android:name=".SiNong.SApproveTeacherShow" />
        <activity android:name=".XiaoHui.XiuGaiActivity"></activity>
    </application>

</manifest>